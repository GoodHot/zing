<seelog type="asynctimer" asyncinterval="5000000" minlevel="debug" maxlevel="error">
	<outputs formatid="main">
		<console/>
		<rollingfile type="size" filename="./log/roll.log" maxsize="10240" maxrolls="5" />
	</outputs>
	<formats>
		<format id="main" format="%Date %Time [%Level] %Func %Msg%n"/>
	</formats>
</seelog>